package com.adobe.washuhci.interactivehist.timeline {		import flash.text.TextField;		public class GenericMarker extends TimelineMarker {			private var _txtLabel:TextField;			public function GenericMarker() {			super();			_txtLabel = this.getChildByName("txtLabel") as TextField;		}				public override function set text(value:String):void {			super.text = value;			_txtLabel.text = value;		}		}	}